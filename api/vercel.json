{
  "version": 2,
  "builds": [
    {
      "src": "functions/index.ts",
      "use": "@vercel/node",
      "config": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/functions/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/health?warmer=true",
      "schedule": "*/5 * * * *"
    }
  ]
} 