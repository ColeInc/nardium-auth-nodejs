{
  "version": 2,
  "builds": [
    {
      "src": "functions/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/functions/index.ts"
    }
  ],
  "functions": {
    "functions/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/health?warmer=true",
      "schedule": "*/5 * * * *"
    }
  ]
} 