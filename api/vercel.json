{
  "version": 2,
  "builds": [
    {
      "src": "functions/index.ts",
      "use": "@vercel/node",
      "config": {
        "memory": 1024,
        "maxDuration": 20
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/functions/index.ts",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/functions/index.ts"
    }
  ]
} 